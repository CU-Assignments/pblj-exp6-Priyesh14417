class Student:
    def __init__(self, name, marks):
        self.name = name
        self.marks = marks

    def __repr__(self):
        return f"Student(name={self.name}, marks={self.marks})"

students = [
    Student("Priyesh", 88),
    Student("Diwam", 72),
    Student("Krishna", 90),
    Student("Dharmesh", 65),
    Student("Ankit", 78)
]

filtered_students = filter(lambda x: x.marks > 75, students)

sorted_students = sorted(filtered_students, key=lambda x: x.marks, reverse=True)

sorted_student_names = [student.name for student in sorted_students]
print("Students scoring above 75% :", sorted_student_names)
